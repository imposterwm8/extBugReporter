{
  "manifest_version": 3,
  "name": "IssuetrakQAI",
  "version": "1.9",
  "background": {
    "service_worker": "background.js"
  },
  "description": "AI-powered QA analysis for React applications with intelligent bug reporting and performance insights.",
  "permissions": [
    "activeTab",
    "scripting",
    "clipboardWrite",
    "storage"
  ],
  "host_permissions": [
    "https://generativelanguage.googleapis.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "options_page": "options.html"
}
