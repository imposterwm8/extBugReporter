{
  "manifest_version": 3,
  "name": "IssuetrakQAI",
  "version": "2.0",
  "background": {
    "service_worker": "background.js"
  },
  "description": "AI-powered QA analysis for React applications with intelligent bug reporting and performance insights.",
  "permissions": [
    "activeTab",
    "scripting",
    "clipboardWrite",
    "storage"
  ],
  "host_permissions": [
    "https://generativelanguage.googleapis.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html"
}
